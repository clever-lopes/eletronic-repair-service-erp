<div id="<%= dom_id board %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Modelo:</strong>
    <%= board.model %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Número de série:</strong>
    <%= board.serial_number %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Quantidade:</strong>
    <%= board.quantity %>
  </p>

</div>
